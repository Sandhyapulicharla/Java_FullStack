package javaBasics;

public class AreaOfFigures {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			//area of reactangle
			float l=9.4f;
			float b=8.3f;
			float area;
			
			area=l*b;
		System.out.println("Area of reactagle of length="+l+" and breadth="+b+" is "+area);


		
	}

}
